{ 
class Program 
{ 
public static void Main(string[] args) 
{ 
Console.WriteLine("Введите число "); 
int n = Convert.ToInt32(Console.ReadLine()); 
for (int i = 2; i <= n; i++) 
{ 
if (i % 2 != 0) 
if (i % 3 != 0) 
if (i % 5 != 0) 
if (i % 7 != 0) 
Console.WriteLine(i); 
} 
Console.ReadKey(); 
} 
} 
}
